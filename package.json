{
  "name": "openhrm_playwright_typescript",
  "version": "1.0.0",
  "description": "boiler plate framework for playwright using typescript",
  "main": "index.js",
  "scripts": {
    "test:qa": "set NODE_ENV=qa && set SALT=PULKIT12 && npx playwright test",
    "test": "ts-node src/tests/sampleTest.spec.ts",
    "clean": "rm -rf dist",
    "lint": "eslint --config eslint.config.mjs .",
    "lint:fix": "eslint --config eslint.config.mjs . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
   "check:all": "eslint --config eslint.config.mjs . --fix && prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\""
  },
  "keywords": [],
  "author": "Veersa Technologies",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@faker-js/faker": "^8.4.1",
    "@playwright/test": "^1.45.3",
    "@types/crypto-js": "^4.2.2",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^22.0.0",
    "dotenv": "^16.4.5",
    "eslint": "^9.8.0",
    "globals": "^15.8.0",
    "ts-jest": "^29.2.3",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "crypto-js": "^4.2.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "moment-timezone": "^0.5.45",
    "ts-node": "^10.9.2",
    "winston": "^3.13.1"
  }
}
